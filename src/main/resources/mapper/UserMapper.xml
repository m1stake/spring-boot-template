<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.songfeifan.samples.sbt.dao.UserMapper">

    <select id="loadByUsername" resultType="com.songfeifan.samples.sbt.model.entity.UserDO">
        select *
        from sys_user
        where username = #{username}
    </select>

</mapper>



